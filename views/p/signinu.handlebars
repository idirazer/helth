<div class="col-md-4 offset-md-4" style="padding-top: 30px;">
{{#each masseges}}
<div class="alert alert-danger"style=" margin-left:25px;width:350px;">
    <p>{{this}}</p>
</div>
    
{{/each}}

<form action="signin" method="post" class="formsin" style="width: 450px; height:350px;">
    <label for="" style="font-size: 30px;margin-top:10px;">Sign in</label>
    <div class="form-group " style="margin-bottom:20px;"> 
        <input type="email" name="email" placeholder="enter your Email" class="form-control" style="height:50px;">
    </div>
    <div class="form-group "> 
        <input type="password" name="password" placeholder="enter your Password" class="form-control" style="height:50px;">
    </div>
    
    <button type="submit" class="btn btn-primary">SignIn</button>
    <p class="message" style="margin-top: 20px;font-size:15px;">Not registered? <a href="/users/signup">Create an account</a></p>
</form>
</div>
