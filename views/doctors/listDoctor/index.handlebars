<!-- TEAM -->
<section id="team" data-stellar-background-ratio="1" style="background-color: #7468680d;">
  <div class="container">
    <div class="row" style="
            background-color: white;
            -webkit-box-shadow: -2px 3px 22px 6px rgba(0, 0, 0, 0.71);
            -moz-box-shadow: -2px 3px 22px 6px rgba(0, 0, 0, 0.71);
            box-shadow: -2px 3px 22px 6px rgba(0, 0, 0, 0.71);
          ">
      <div class="col-md-6 col-sm-6">
        <div class="about-info">

          <h2 class="wow fadeInUp" data-wow-delay="0.1s">{{# each namespecialty}}{{this.name_of_specialty}}{{/each}}
          </h2>

        </div>
      </div>
      <div class="clearfix"></div>

      {{# each doctors}}
      <div class="col-md-3 col-sm-6" style="padding-bottom: 15px;">
        <div class="team-thumb wow fadeInUp" data-wow-delay="0.4s" style="height: 610px; overflow: scroll;">
          <a href="/each_doctor/{{this._id}}">
            <img src="/picturs/speciality/team-image2.jpg" class="img-responsive" alt="" />
          </a>
          <div class="team-info">
            <h3>{{this.name}}</h3>
            <p>{{# each namespecialty}}{{this.name_of_specialty}}{{/each}}</p>
            <div class="team-contact-info">
              <p style="padding-top: 5px;">
                <i class="fa fa-phone"></i> 010-070-0170
              </p>
              <p style="padding-top: 5px;">
                <i class="fa fa-envelope-o"></i>
                <a href="#">{{this.email}}</a>
              </p>
              <p style="padding-top: 5px;">
                <i class="fas fa-map-marker-alt"></i>{{this.address}}
              </p>
              <div style="padding: 10px;">
                <i class="fas fa-star fa-2x" data-index="0" id="1"></i>
                <i class="fas fa-star fa-2x" data-index="1" id="1"></i>
                <i class="fas fa-star fa-2x" data-index="2" id="1"></i>
                <i class="fas fa-star fa-2x" data-index="3" id="1"></i>
                <i class="fas fa-star fa-2x" data-index="4" id="1"></i>
              </div>
            </div>
            <ul class="social-icon">
              <li><a href="#" class="fa fa-facebook-square"></a></li>
              <li><a href="#" class="fa fa-envelope-o"></a></li>
              <li><a href="#" class="fa fa-flickr"></a></li>
            </ul>
          </div>
        </div>
      </div>
      {{/each}}


    </div>
    <div class="row " style="margin-top: 40px; background-color: white;">
      <div class="col-md-12 ">

        <div id="carousel3" class="row2">
          {{# each secendnames}}

          <div class="item">
            <div>
              <div class="thumbnail noborder">
                <a href="">
