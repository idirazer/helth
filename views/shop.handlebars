    {{#each products}}
    <div class="row">
      {{#each this}}
      <div class="col-sm-4">
        <div class="card">
          <div class="text-center">
            <img src="{{this.imgePath}}" class="card-img-top" alt="...">
          </div>
          <div class="card-body">
            <h5 class="card-title">{{this.productName}}</h5>
            <p class="card-text">
              Medicine Name: {{this.information.utilisation}} <br>
              Form :{{this.information.consigne}} </p>
            Dosage :{{this.information.Quantum}} <br>
            Quantity : {{this.information.quantite}} <br>

            <div>
              <div class="price float-left"> {{this.price}}DA</div>

              {{#if ../../chekuse}}
              <a href="/addTocart/{{this._id}}/{{this.price}}/{{this.productName}}"
                class="btn btn-info float-right">Demand</a>

              {{else}}
              <button type="button" class="btn btn-info float-right" data-toggle="modal"
                data-target="#exampleModalCenter">
                Demand
              </button>
              {{/if}}


            </div>
          </div>
        </div>
      </div>

      {{/each}}
    </div>

    {{/each}}

